{
    "name": "glen/slack-unfurl-jira",
    "description": "Jira links unfurler for slack-unfurl",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JiraSlackUnfurl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JiraSlackUnfurl\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "chobie/jira-api-restclient": "^2.0@dev"
    },
    "conflict": {
        "glen/slack-unfurl": "<0.9.0"
    },
    "require-dev": {
        "glen/slack-unfurl": "^0.9.0",
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^4.0"
    }
}
